{
    "name": "juanparati/csvreader",
    "description": "A light/fast CSV reader suitable for pretty large files.",
    "type": "library",
    "license": "MIT",
    "tags": ["csv", "reader", "quick", "stream"],
    "authors": [
        {
            "name": "Juan Lago",
            "email": "juanparati@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.3",
        "ext-mbstring": "*",
        "ext-ctype": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "ext-json": "*",
        "phpbench/phpbench": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Juanparati\\CSVReader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juanparati\\CSVReader\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "bench": "phpbench run tests/Benchmark --report=default",
        "test": "phpunit"
    }
}
